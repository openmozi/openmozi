{
  "name": "mozi-bot",
  "version": "1.6.2",
  "description": "支持国产模型和国产通讯软件的智能助手机器人",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "mozi": "./dist/cli/index.js"
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli/index.ts",
    "start": "node dist/cli/index.js start",
    "start:gateway": "tsx src/gateway/server.ts",
    "cli": "node dist/cli/index.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "chatbot",
    "ai",
    "llm",
    "deepseek",
    "minimax",
    "kimi",
    "moonshot",
    "stepfun",
    "feishu",
    "dingtalk",
    "qq",
    "lark",
    "chinese",
    "agent"
  ],
  "author": "King-Chau",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/King-Chau/mozi.git"
  },
  "homepage": "https://github.com/King-Chau/mozi#readme",
  "bugs": {
    "url": "https://github.com/King-Chau/mozi/issues"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@larksuiteoapi/node-sdk": "^1.56.1",
    "@sinclair/typebox": "^0.34.48",
    "axios": "^1.13.4",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "crypto-js": "^4.2.0",
    "dingtalk-stream": "^2.1.4",
    "dotenv": "^16.4.5",
    "eventsource-parser": "^3.0.0",
    "express": "^4.21.0",
    "glob": "^13.0.0",
    "json5": "^2.2.3",
    "nanoid": "^5.0.7",
    "node-cache": "^5.1.2",
    "openai": "^4.67.1",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "playwright-core": "^1.58.0",
    "qq-guild-bot": "^2.9.5",
    "ws": "^8.18.0",
    "yaml": "^2.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/node": "^22.7.4",
